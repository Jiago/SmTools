<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.centerm.yuhc.smtools">

    <application
        android:allowBackup="true"
        android:icon="@drawable/desktop"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:windowSoftInputMode="stateUnchanged|adjustPan"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitymanager.AppVersionActivity"/>
        <activity android:name=".activitymanager.IntroductionActivity"/>
    </application>

</manifest>
